{
  "name": "matchcare-backend",
  "version": "1.0.0",
  "description": "MatchCare - Ontology-Based Skincare Recommendation System Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "setup": "node scripts/setup-database.js",
    "import-data": "node scripts/import-data.js",
    "seed": "node scripts/seed-data.js",
    "migrate-images": "node scripts/migrate-images.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf node_modules package-lock.json",
    "reset-db": "npm run setup && npm run import-data && npm run seed"
  },
  "keywords": [
    "skincare",
    "recommendation", 
    "ontology",
    "nodejs",
    "postgresql",
    "api"
  ],
  "author": "MatchCare Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0", 
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "sequelize": "^6.35.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "csvtojson": "^2.0.10",
    "n3": "^1.17.2",
    "natural": "^6.8.0",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "axios": "^1.6.2",
    "multer": "^1.4.5",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}